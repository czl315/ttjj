<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ttjj.dao.mapper.TestMapper">
    <select id="selectBlog" parameterType="int" resultType="ttjj.dto.Fupan">
        select * from fupan where id = #{id}
    </select>
    <select id="selectCount" resultType="Integer">
        select count(1) from fupan
    </select>
</mapper>